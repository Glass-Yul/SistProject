package day0104;

import java.util.Scanner;

public class Back10807 {

	public static void main(String[] args) {
		// 총 N개의 정수가 주어졌을 때, 정수 v가 몇 개인지 구하는 프로그램
		Scanner sc = new Scanner(System.in);
		
		int num = sc.nextInt();
		int arr[] = new int[num];
		
		for(int i=0;i<num;++i)
			arr[i]=sc.nextInt();
		
		num = sc.nextInt();
		int count=0;
		for(int i=0;i<num;++i){
			count++;
			if(num==arr[i]) break;
		}
		System.out.println(count);
	}

}
